<?xml version="1.0" encoding="UTF-8"?>
<json-schema name="npm.package" xmlns="https://www.json.org">
	<string name="name" />
	<string name="version" />
	<string name="description" />
	<string name="main" />
	<typed-object name="scripts">
		<base>string</base>
	</typed-object>
	<object name="repository">
		<string name="type" />
		<string name="url" />
	</object>
	<string name="author" />
	<string name="license" />
	<object name="bugs">
		<string name="url" />
	</object>
	<typed-object name="devDependencies">
		<base>string</base>
	</typed-object>
	<typed-object name="dependencies">
		<base>string</base>
	</typed-object>
</json-schema>