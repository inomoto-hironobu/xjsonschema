<?xml version="1.0" encoding="UTF-8"?>
<json-schema name="npm.package" xmlns="https://www.json.org/typeb">
	<string name="name" />
	<string name="version" />
	<string name="description" />
	<string name="main" />
	<object name="scripts">
		<type value="string"/>
	</object>
	<object name="repository">
		<string name="type" />
		<string name="url" />
	</object>
	<string name="author" />
	<string name="license" />
	<object name="bugs">
		<string name="url" />
	</object>
	<object name="devDependencies">
		<type value="string"/>
	</object>
	<object name="dependencies">
		<type value="string"/>
	</object>
</json-schema>